// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Module {
  id                    Int             @id @default(autoincrement())
  name                  String          @unique
  type                  String  
  city                  String
}

model Detail {
  id                    Int             @id @default(autoincrement())
  value                 Int
  duration              Int
  number                Int
  state                 Boolean
}

model Log {
  id                    Int             @id @default(autoincrement())
  date                  DateTime
  valueSave             Int
}